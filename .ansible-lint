---
#
# Ansible managed
#
exclude_paths:
  - meta/preferences.yml
  - molecule/default/prepare.yml
  - molecule/default/converge.yml
  - molecule/default/verify.yml
  - molecule/default/collections.yml
  - .tox
  - .cache
  - .github
  - requirements.yml

skip_list:
  - yaml[truthy]
  - '106'  # Role name {} does not match \^[a-z][a-z0-9_]+$\ pattern'

enable_list:
  - name[prefix]
